{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "public/assets/js",
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "public/assets/js/unused.js"],
  "exclude": ["node_modules"]
}
